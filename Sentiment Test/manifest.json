{
    "manifest_version": 2,
    "name": "Gmail Scraper Extension",
    "version": "1.0",
    "description": "Chrome extension to scrape Gmail",
    "permissions": [
      "https://www.googleapis.com/",  
      "identity",
      "storage"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "oauth2": {
      "client_id": "614787254411-hnho0ltvgbeb6255sp9ufo0m5ofu39oj.apps.googleusercontent.com",
      "scopes": [
        "https://www.googleapis.com/auth/gmail.readonly"
      ]
    },
    "icons": {
      "48": "icon.png"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [],
    "web_accessible_resources": [
      "popup.js"
    ]
  }
  